{
    "name": "jankx-woocommerce-blocks",
    "version": "1.0.0",
    "description": "WooCommerce Blocks for Jankx Framework - Fork and extension of WooCommerce Blocks",
    "main": "src/index.js",
    "scripts": {
        "build": "wp-scripts build --config webpack.config.js",
        "dev": "wp-scripts start --config webpack.config.js",
        "watch": "wp-scripts start",
        "lint:js": "wp-scripts lint-js",
        "lint:css": "wp-scripts lint-style",
        "format": "wp-scripts format",
        "test": "wp-scripts test-unit-js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jankx/woocommerce-blocks.git"
    },
    "keywords": [
        "wordpress",
        "woocommerce",
        "gutenberg",
        "blocks",
        "jankx"
    ],
    "author": "Puleeno Nguyen <puleeno@gmail.com>",
    "license": "GPL-3.0-or-later",
    "devDependencies": {
        "@wordpress/scripts": "^27.9.0",
        "@wordpress/eslint-plugin": "^17.13.0",
        "@wordpress/stylelint-config": "^21.38.0"
    },
    "dependencies": {
        "@wordpress/blocks": "^12.29.0",
        "@wordpress/block-editor": "^12.20.0",
        "@wordpress/components": "^25.16.0",
        "@wordpress/data": "^9.22.0",
        "@wordpress/element": "^5.29.0",
        "@wordpress/i18n": "^4.52.0",
        "@wordpress/icons": "^9.43.0",
        "@wordpress/api-fetch": "^6.49.0",
        "@wordpress/html-entities": "^3.52.0",
        "@wordpress/url": "^3.53.0",
        "@wordpress/compose": "^6.29.0",
        "@wordpress/hooks": "^3.52.0",
        "@wordpress/keycodes": "^3.52.0",
        "@wordpress/viewport": "^5.29.0"
    },
    "browserslist": [
        "extends @wordpress/browserslist-config"
    ],
    "eslintConfig": {
        "extends": [
            "@wordpress/eslint-plugin/recommended"
        ]
    },
    "stylelint": {
        "extends": [
            "@wordpress/stylelint-config/scss"
        ]
    }
}
